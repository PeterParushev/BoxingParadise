@model dynamic

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Upcoming boxing matches</h2>
<table class=".table-striped .table-hover .table-bordered">
    <thead>
        <tr>
            <td>
                Name of first boxer
            </td>
            <td>
                Name of second boxer
            </td>
            <td>
                Name of venue
            </td>
            <td>
                StartDate
            </td>
            <td>
                Description
            </td>
            <td>
                Winner
            </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var match in ViewBag.Matches)
        {
            <tr>
                <td>
                    @match.BoxerDtoOneId
                </td>
                <td>
                    @match.BoxerDtoTwoId
                </td>
                <td>
                    @match.VenueDtoId
                </td>
                <td>
                    @match.Date
                </td>
                <td>
                    @match.Description
                </td>
                <td>
                    @match.WinnerId
                </td>
            </tr>
        }
    </tbody>
</table>